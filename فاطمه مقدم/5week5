def myFunc(aa):
    listNameStudent = []
    listKeys = list(aa.keys())
    for item in listKeys:
        grades = aa.get(item)
        count = 0
        for i in grades:
            if i>78:
                count = count+1

        if count== len(grades):
            listNameStudent.append(item)
    return listNameStudent
